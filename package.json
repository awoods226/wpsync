    "react-router-dom": "^4.3.1",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.2",
    "superagent": "^3.8.3",
    "wpapi": "^1.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
